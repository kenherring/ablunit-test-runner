{
	// "eslint.lintTask.enable": true,
	"eslint.codeActionsOnSave.mode": "problems",
	// "extension-test-runner.extractSettings": {
	// 	"suite": [ "suite" ],
	// 	"test": [ "test" ],
	// 	// "extractWith": "syntax",
	// 	"extractWith": "evaluation",
	// },
	"git.branchProtection": [
		"main"
	],
	// "extension-test-runner.extractSettings": {
	// 	"suite": [
	// 		"describe",
	// 		"suite"
	// 	],
	// 	"test": [
	// 		"it",
	// 		"test"
	// 	],
	// 	"extractWith": "syntax"
	// },
	"files.associations": {
		"ablunit-test-profile*.json": "jsonc"
	},
	"files.exclude": {
		"./dist/**": true,
		".worktrees/": true
	},
	"files.watcherExclude": {
		".git/objects": true,
		".git/subtree-cache": true,
		".vscode-test": true,
		"coverage": true,
		"dist": true,
		"out": true,
		"node_modules": true
	},
	"json.schemas": [
		{
			"fileMatch": [
				"ablunit-test-profile*.json",
				"ablunit-test-profile*.jsonc",
			],
			"url": "./resources/ablunit-test-profile.schema.json"
		}
	],
	"npm.autoDetect": "on",
	"redhat.telemetry.enabled": false,
	"search.exclude": {
		".vscode-test": true,
		"coverage": true,
		"dist": true,
		"out": true,
		"node_modules": true,
		"dummy-ext/.vscode-test/**": true,
		"resources/ADE": true,
	},
	"shellcheck.useWorkspaceRootAsCwd": true,
	"sonarlint.connectedMode.project": {
		"connectionId": "sonarcloud-kenherring",
		"projectKey": "kenherring_ablunit-test-runner"
	},
	"sonarlint.disableTelemetry": true,
	"task.autoDetect": "on",
	"typescript.preferences.quoteStyle": "single",
    "typescript.validate.enable": true,
    "circleci.filters.branchFilter": "allBranches",
    "circleci.persistedProjectSelection": [
		"gh/kenherring/ablunit-test-runner"
	],
}
