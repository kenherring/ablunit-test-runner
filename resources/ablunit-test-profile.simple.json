{
	"configurations": [
		{
			"tempDir": "${workspaceFolder}",
			"importOpenedgeProjectJson": true,
			"timeout": 30000,
			"command": {
				"executable": "_progres",
				"progressIni": "progress.ini",
				"batch": true,
				"debugHost": "localhost",
				"debugPort": 3199,
				"additionalArgs": []
			},
			"options": {
				"output": {
					"location": "${tempDir}",
					"filename": "results",
					"format": "xml",
					"updateFile": "updates.log"
				},
				"debug": false,
				"quitOnEnd": true,
				"writeLog": false,
				"showErrorMessage": true,
				"throwError": true,
				"xref": {
					"useXref": false,
					"xrefLocation": "${tempDir}",
					"xrefExtension": "xml.xref",
					"xrefThrowError": false
				}
			},
			"profiler": {
				"enabled": true,
				"description": "Run via VSCode - ABLUnit Test Runner Extension",
				"filename": "prof.out"
			}
		}
	]
}
