{
	"name": "g",
	"displayName": "ablunit-test-provider",
	"description": "Integrate OpenEdge ABLUnit Tests with VSCode",
	"version": "0.0.1",
	"publisher": "kenherring",
	"private": true,
	"license": "MIT",
	"repository": "https://github.com/kherring/ablunit-test-provider",
	"enabledApiProposals": [
		"testCoverage"
	],
	"engines": {
		"vscode": "^1.68.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/*.cls"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./",
		"download-api": "dts main && dts dev",
		"postinstall": "npm run download-api"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"@vscode/dts": "^0.4.0",
		"eslint": "^8.26.0",
		"typescript": "^5.2.2"
	},
	"configuration": {
		"type": "object",
		"title": "ABLUnit Command",
		"properties": {
			"ablunit.runTestCommand": {
				"type": "string",
				"default": "",
				"description": "When enabled outline shows list of include files"
			}
		}
	},
	"dependencies": {
		"xml2js": "^0.6.2"
	}
}
