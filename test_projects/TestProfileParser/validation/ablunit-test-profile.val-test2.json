{
	"configurations": [
		{
			"hide": false,
			"tempDir": "${workspaceFolder}",
			"importOpenedgeProjectJson": true,
			"command": {
				"executable": "_progres",
				"progressIni": "progress.ini",
				"batch": true,
				"additionalArgs": []
			},
			"options": {
				"output": {
					"location": "${tempDir}",
					"filename": "results",
					"format": "xml"
				},
				"quitOnEnd": true,
				"writeLog":  false,
				"showErrorMessage": true,
				"throwError": true
			},
			"profiler": {
				"enabled": true,
				"coverage": true,
				"description": "Run via VSCode - ABLUnit Test Provider Extension",
				"filename": "prof.out",
				"listings": false,
				"statistics": false,
				"traceFilter": "",
				"tracing": false,
				"writeJson": false
			}
		}
	]
}
